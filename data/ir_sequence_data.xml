<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for IT Service Types -->
        <record id="seq_it_service_type" model="ir.sequence">
            <field name="name">IT Service Type Sequence</field>
            <field name="code">it.service.type</field>
            <field name="prefix">SRV/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for IT Service Requests -->
        <record id="seq_it_service_request" model="ir.sequence">
            <field name="name">IT Service Request Sequence</field>
            <field name="code">it.service.request</field>
            <field name="prefix">REQ/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for IT Contracts -->
        <record id="seq_it_contract" model="ir.sequence">
            <field name="name">IT Contract Sequence</field>
            <field name="code">it.contract</field>
            <field name="prefix">CONT/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo> 